<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/login_navigation"
        app:startDestination="@id/signInFragment">
    <fragment
            android:id="@+id/signInFragment"
            android:name="com.magic.Owners.presentation.ui.auth.signin.SignInFragment"
            android:label="SignInFragment">
        <action
                android:id="@+id/action_signInFragment_to_nav_graph"
                app:destination="@id/mainActivity" />
    </fragment>

    <activity
            android:id="@+id/mainActivity"
            android:name="com.magic.Owners.presentation.ui.MainActivity"
            android:label="MainActivity" />

    <fragment  android:id="@+id/signUpFragment"
            android:name="com.magic.Owners.presentation.ui.auth.signup.SignUpFragment"
            android:label="SignUpFragment"/>
</navigation>